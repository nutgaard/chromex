{
  "manifest_version": 2,

  "name": "DevCenterApp",
  "description": "All notifications for developers",
  "version": "0.0.1",

  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "storage",
    "management",
    "http://stash.devillo.no/*",
    "https://stash.devillo.no/*",
    "http://jira.adeo.no/*",
    "https://jira.adeo.no/*"
  ]
}